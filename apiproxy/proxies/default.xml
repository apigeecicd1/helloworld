<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response>
      <Step>
        <Name>add-cors</Name>
      </Step>
      <Step>
        <Name>ResponsePayload</Name>
      </Step>
    </Response>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/helloworld</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="noroute"/>
</ProxyEndpoint>
